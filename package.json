{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "simple webpack boiler plate for es6, react, sass, less, stylus",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --colors",
    "test": "mocha --compilers js:babel-core/register --require ./test/index.js --recursive",
    "test:watch": "npm run test -- --watch",
    "build:dev": "webpack",
    "build": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://wonsong82@bitbucket.org/wonsong82/webpack-boilerplate.git"
  },
  "homepage": "https://bitbucket.org/wonsong82/webpack-boilerplate#readme",
  "keywords": [
    "webpack",
    "boilerplate",
    "react",
    "react-hot",
    "css",
    "less",
    "sass",
    "stylus, es6",
    "polyfill",
    "webpack-dev-server"
  ],
  "author": "Won Song",
  "license": "ISC",

  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "connect-with-transition-group": "^0.1.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "latest",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-scrollbar": "^0.4.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "velocity-animate": "^1.2.3",
    "velocity-react": "^1.1.5"
  }
}
